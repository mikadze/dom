    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "argon2": "^0.41.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0",
